public static bool IsPrime(int n)
        {
            bool isPrime = true;
            if (n>1)
            {
                for (int i = 2; i < n; i++)
                {
                    if (n % i == 0)
                    {
                        isPrime = false;
                        break;
                    }
                }
            }
            else
            {
                isPrime = false;
            }


            return isPrime;
        }
